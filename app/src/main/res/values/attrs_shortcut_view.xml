<resources>
    <declare-styleable name="ShortcutBoxView">
        <attr name="shortcutTitle" format="string" />
        <attr name="shortcutIcon" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="CustomAudioIcon">
        <attr name="type">
            <enum name="start" value="0" />
            <enum name="forward" value="1" />
            <enum name="backward" value="2" />
            <enum name="exit" value="3" />
            <enum name="mode" value="4" />
        </attr>
        <attr name="colors" format="color" />
    </declare-styleable>
</resources>
